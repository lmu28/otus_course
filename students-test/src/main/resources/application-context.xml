<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="questionsResource" class="org.springframework.core.io.ClassPathResource">
        <constructor-arg value="questions.csv"/>
    </bean>

    <bean id="questionFileService" class="org.student.testing.service.QuestionFileServiceImpl">
        <constructor-arg name="classPathResource" ref="questionsResource" />
    </bean>


</beans>
